  height: 25px;
  width: 25px;
  background: transparent !important;
  animation: rotate 0.75s 0s linear infinite;
}

@-webkit-keyframes rotate {
  0% {
    transform: rotate(0deg) scale(1);
  }

  100% {
    transform: rotate(360deg) scale(1);
  }
}

@keyframes rotate {
  0% {
    transform: rotate(0deg) scale(1);
  }

  100% {
    transform: rotate(360deg) scale(1);
  }
}

